# lamdabuilder(1) completion                                       -*- shell-script -*-

_lamdabuilder() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    opts="shell info clean stop start delete deploy help create-image -t --test-data -a --aws-dir -n --node -p --production -h --help --no-colors"

    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
}
complete -F _lamdabuilder lamdabuilder
